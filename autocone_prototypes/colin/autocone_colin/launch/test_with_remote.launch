<?xml version="1.0"?>
<launch>

  <arg name="max_speed" default="0" />

  <!-- Open camera -->

  <!-- Start communication with xbox controller -->
  <node pkg="joy" type="joy_node" name="joy_node"/>
  <node pkg="autocone_colin_decision" type="line_follower.py" name="line_follower" 
        args="-m $(arg max_speed)" output="screen"/>

  <!-- Start serial communication with arduino -->
  <node pkg="autocone_colin_serial_communication" type="serial_communication.py" name="arduino_serial_comm" output="screen"/>

</launch>
